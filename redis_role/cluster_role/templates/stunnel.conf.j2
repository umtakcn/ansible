cert = {{ cert_path }}
pid = {{ pid }}
output= {{ output }}
fips=no

sslVersion=all
options=NO_SSLv2
options=NO_SSLv3
options=NO_TLSv1

# Inbound Redis Worker Port

[redis-server-1.1]
client=no
accept = {{ inbound_worker_accept }}
connect = {{ inbound_worker_connect }}

[redis-server-1.2]
client=no
accept = {{ inbound_worker_acceptC }}
connect = {{ inbound_worker_connectC }}


# Inbound Redis Gossip Port

[redis-server-gossip-1.1]
client=no
accept = {{ inbound_gossip_acceptC }}
connect = {{ inbound_gossip_connectC }}

[redis-server-gossip-1.2]
client=no
accept = {{ inbound_gossip_acceptC2 }}
connect = {{ inbound_gossip_connectC2 }}


# Outbound Redis Worker Port

[redis-client-2.1]
client=yes
accept = {{ outbound_worker_accept }}
connect = {{ outbound_worker_connect }}

[redis-client-2.2]
client=yes
accept = {{ outbound_worker_acceptC }}
connect = {{ outbound_worker_connectC }}

[redis-client-3.1]
client=yes
accept = {{ outbound_worker_accept2 }}
connect = {{ outbond_worker_connect2 }}

[redis-client-3.2]
client=yes
accept = {{ outbound_worker_acceptC2 }}
connect = {{ outbond_worker_connectC2 }}

# Outbound Redis Gossip Port

[redis-client-gossip-2.1]
client=yes
accept = {{ outbound_gossip_acceptC }}
connect = {{ outbound_gossip_connectC }}

[redis-client-gossip-2.2]
client=yes
accept = {{ outbound_gossip_acceptC2 }}
connect = {{ outbound_gossip_connectC2 }}

[redis-client-gossip-3.1]
client=yes
accept = {{ outbound_gossip_acceptC3 }}
connect = {{ outbound_gossip_connectC3 }}

[redis-client-gossip-3.2]
client=yes
accept = {{ outbound_gossip_acceptC4 }}
connect = {{ outbound_gossip_connectC4 }}
